{
  "name": "stratos",
  "version": "2.0.0",
  "description": "Stratos Console",
  "main": "index.js",
  "scripts": {
    "build-backend": "gulp build-backend",
    "cf-get-backend-deps": "gulp cf-get-backend-deps",
    "cf-build-backend": "gulp cf-build-backend",
    "bosh-build-backend": "gulp bosh-build-backend",
    "deploy-cf": "gulp deploy:cf",
    "test-backend": "gulp test-backend",
    "update-webdriver": "webdriver-manager update",
    "build": "ng build --prod --aot=false --build-optimizer=false",
    "build-cf": "node --max_old_space_size=864 node_modules/@angular/cli/bin/ng build --prod --aot=false --build-optimizer=false",
    "build-dev": "ng build --dev",
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.js --ssl 1 --ssl-key dev-ssl/server.key --ssl-cert dev-ssl/server.crt",
    "start-prod": "ng serve --prod --aot=false --sourcemaps=true --proxy-config proxy.conf.js --ssl 1 --ssl-key dev-ssl/server.key --ssl-cert dev-ssl/server.crt",
    "test": "ng test --code-coverage --sourcemaps=false -sr",
    "test-debug": "ng test --sourcemaps=false",
    "codecov": "codecov -f coverage/*.json",
    "lint": "ng lint --type-check --format stylish",
    "sass-lint": "sass-lint -v",
    "e2e": "ng e2e --proxy-config proxy.conf.js --ssl 1 --ssl-key dev-ssl/server.key --ssl-cert dev-ssl/server.crt",
    "headless-e2e": "xvfb-run --server-args='-screen 0 1920x1080x24' protractor ./protractor.conf.js",
    "climate": "codeclimate analyze $(git diff --name-only master)",
    "gate-check": "npm run lint && npm run test-debug -- --browsers ChromeHeadless --single-run"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/animations": "^5.1.1",
    "@angular/cdk": "^5.2.1",
    "@angular/common": "^5.1.1",
    "@angular/compiler": "^5.1.1",
    "@angular/core": "^5.1.1",
    "@angular/forms": "^5.1.1",
    "@angular/http": "^5.1.1",
    "@angular/material": "^5.2.1",
    "@angular/platform-browser": "^5.1.1",
    "@angular/platform-browser-dynamic": "^5.1.1",
    "@angular/platform-server": "^5.1.1",
    "@angular/router": "^5.1.1",
    "@ngrx/effects": "^4.1.1",
    "@ngrx/router-store": "^4.1.1",
    "@ngrx/store": "^4.1.1",
    "@ngrx/store-devtools": "^4.1.1",
    "@swimlane/ngx-charts": "^7.1.1",
    "angular2-virtual-scroll": "^0.3.1",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "hyperlist": "^1.0.0-beta",
    "moment": "^2.19.1",
    "ngrx-store-logger": "^0.2.0",
    "normalizr": "^3.2.3",
    "queueing-subject": "^0.1.1",
    "reselect": "^3.0.1",
    "rxjs": "^5.5.5",
    "rxjs-spy": "^6.1.0",
    "rxjs-websockets": "^4.0.0",
    "web-animations-js": "^2.3.1",
    "xterm": "^2.9.1",
    "zone.js": "^0.8.14"
  },
  "engines": {
    "node": "8.x.x"
  },
  "devDependencies": {
    "delete": "^0.3.2",
    "gulp": "^4.0.0",
    "lodash": "^4.13.1",
    "fs-extra": "^3.0.1",
    "mktemp": "^0.4.0",
    "stratos-merge-dirs": "^0.2.1",
    "q": "^1.4.1",
    "@angular/cli": "^1.6.1",
    "@angular/compiler-cli": "^5.1.1",
    "@angular/language-service": "^4.4.6",
    "@types/jasmine": "^2.6.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/karma": "^1.7.1",
    "@types/node": "~6.0.60",
    "@types/xterm": "^2.0.3",
    "codecov": "^3.0.0",
    "codelyzer": "~3.1.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-spec-reporter": "0.0.31",
    "ngrx-store-freeze": "^0.2.0",
    "protractor": "~5.1.2",
    "sass-lint": "^1.12.1",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "2.4.2"
  }
}
